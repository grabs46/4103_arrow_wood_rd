<!-- Navbar -->
<nav class="navbar navbar-expand-sm bg-transparent" style="z-index: 1000;">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-lg-6">
                <a href="https://www.redfin.com/TX/Cedar-Park/4103-Arrow-Wood-Rd-78613/home/40183529" id="home-link" class="navbar-brand" style="color:bisque">4103 Arrow Wood Road</a>
            </div>
        </div>
    </div>
</nav>

<!-- Carousel -->  
<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel" style="margin-top: -56px;">
  <div class="carousel-inner">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    {{#each images}}
    <div class="carousel-item {{#if @first}}active{{/if}} align-items-center">
      <img class="d-block w-100" src="{{this.url}}" alt="Slide" style="object-fit: cover; height: 100vh; width: auto;">
      <div class="carousel-caption text-center mb-20" >
        <a href="https://www.redfin.com/TX/Cedar-Park/4103-Arrow-Wood-Rd-78613/home/40183529" class="btn btn-lg btn-outline-light btn-learn-more" style="color: bisque;">Learn More</a>
        <h5>{{this.text}}</h5>
      </div>
    </div>
    {{/each}}
  </div>
  <a class="carousel-control-prev" href="#carouselExampleSlidesOnly" role="button" data-slide="prev" style="position: absolute; top: 50%; transform: translateY(-50%);">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleSlidesOnly" role="button" data-slide="next" style="position: absolute; top: 50%; transform: translateY(-50%);">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<!-- Bootstrap JS -->
<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
